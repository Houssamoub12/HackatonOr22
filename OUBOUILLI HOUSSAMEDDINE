print("how many jobs are avaible")
n = int(input())

s=[]
e=[]
p=[]

s=[1,1,1]#exemple d'une liste pour start date
e=[2,3,4]#exemple d'une liste pour end date 
p=[5,6,4]#exemple d'une liste pour le profit


if n>0 and n<10000

    for i in range(n-1):
        if s[i+1]>=s[i] and s[i+1]<e[i] and p[i+1]>p[i] :
            p = int(p[i+1]) + p
        elif s[i+1]>s[i] :
            p = int(p[i+1]) + p
        else :
            p = int(p[i]) + p
    print(p)
